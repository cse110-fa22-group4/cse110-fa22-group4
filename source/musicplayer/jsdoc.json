{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
      "include": ["./"],
      "exclude": ["node_modules"],
      "includePattern": ".js",
      "excludePattern": ".json"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "./../../docs",
      "recurse": true
    }
}