{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "globals": {
    "jqAPI": true,
    "domAPI": true,
    "fsAPI": true,
    "ffmpegAPI": true,
    "genAPI": true
  },
    "plugins": [
    ],
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "overrides": [
    {
      "files": [
        "*.js",
        "*.json"
      ],
      "rules": {
        "indent": [
          "error",
          4
        ],
        "no-unused-vars": [
          "off"
        ],
        "max-len": [
          "error",
          120
        ]
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
  }
}
